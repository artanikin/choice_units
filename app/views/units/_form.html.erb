<%= form_tag("/", method: 'get') do %>
    <em>Возраст:</em>
    <%= label_tag "age[start]", "от" %>
    <%= text_field_tag "age[start]", nil, size: 3, maxlength: 3 %>
    <%= label_tag "age[stop]", "до" %>
    <%= text_field_tag "age[stop]", nil, size: 3, maxlength: 3 %>
    
    <%= submit_tag "Фильтровать" %>
<% end %>
